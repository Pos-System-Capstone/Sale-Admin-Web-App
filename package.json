  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier -c --write \"src/**/*\"",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.jsx ./src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "start:development": "env-cmd -f .env.development npm run start",
    "start:production": "env-cmd -f .env.production npm run-script start",
    "start:qa": "env-cmd -f .env.qa npm run-script start",
    "build:development": "env-cmd -f .env.development npm run-script build",
    "build:qa": "env-cmd -f .env.qa npm run-script build",
    "build:production": "env-cmd -f .env.production npm run-script build",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
